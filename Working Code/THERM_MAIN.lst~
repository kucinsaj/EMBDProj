00CE 75F00A            145     MOV B,#10D                      ;Store 10D in B ACC
00D1 84                146     DIV AB                          ;Divide ACC by B ACC
00D2 ABF0              147     MOV R3,B                        ;Store remainder in register 3
00D4 75F00A            148     MOV B,#10D                      ;Store 10D in B ACC
00D7 84                149     DIV AB                          ;Divide ACC by B ACC
00D8 120211            150     LCALL ASCII                     ;Convert to ASCII
00DB 1200C1            151     LCALL WRITE                     ;Write to Display
00DE E5F0              152     MOV A,B                         ;Store remainder in ACC
00E0 120211            153     LCALL ASCII                     ;Convert to ASCII
00E3 1200C1            154     LCALL WRITE                     ;Write to Display
00E6 EB                155     MOV A,R3                        ;Store register 3 in ACC
00E7 120211            156     LCALL ASCII                     ;Convert to ASCII
