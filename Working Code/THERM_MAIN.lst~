THERM_MAIN                                                                                                              PAGE 1
                         1     ;;;;;;Assign Port Names;;;;;;
  00B2                   2     RS EQU P3.2
  00B1                   3     RW EQU P3.1
  00B3                   4     E  EQU P3.3
                         5     ;;;;;;Init Variables;;;;;;
  0048                   6     TEMP EQU 48H
  0049                   7     HUM  EQU 49H
                         8     ORG 000H
                         9     ;;;;;;INIT;;;;;;
                        10     INIT:                   ;Program Initialization
0000 900212             11     MOV DPTR,#VALUES                ;Move DPTR to Values LUT
0003 D280               12     SETB P0.0                       ;Set DHT11 Data Port
0005 C2A0               13     CLR P2.0                        ;Clear Output*
0007 758921             14     MOV TMOD,#00100001B             ;Set TMOD
000A 758B00             15     MOV TL1,#00D                    ;Set Timer Low Byte
000D 1201D9             16     LCALL LCD_INIT                  ;Call LCD Init subroutine
0010 12011C             17     LCALL WELCOME                   ;Call Welcome Text
0013 1200B3             18     LCALL CLEARD
0016 120068             19     LCALL DIAGNOSTIC_DISPLAY
                        20     ;LCALL DELAY2
0019 020210             21     LJMP    ENDP
                        22
                        23     MAIN:                   ;Main Routine
001C 7908               24     MOV R1,#8D                      ;Set R1 -> 8D ->08H
001E D280               25     SETB P0.0                       ;Set DHT11 Data Port
0020 C280               26     CLR P0.0                        ;Clear DHT11 Data Port
0022 12007B             27     LCALL DELAY1                    ;Delay
0025 D280               28     SETB P0.0                       ;Set DHT11 Data Port
0027 2080FD             29     WAIT1:JB P0.0,$                 ;Wait till Data Port LOW
002A 3080FD             30     WAIT2:JNB P0.0,$                ;Wait till Data Port HIGH
002D 2080FD             31     WAIT3:JB P0.0,$                 ;Wait till Data Port LOW
                        32
                        33     DATALOOP:               ;DHT11 Data Colection LOOP
0030 3080FD             34     JNB P0.0,DATALOOP               ;Wait till Data Port HIGH
0033 23                 35     RL A                            ;Rotate ACC Left
0034 F8                 36     MOV R0,A                        ;Move ACC to Register 0
0035 D28E               37     SETB TR1                        ;SET Timer 1 RUN
                        38     WAIT4:
0037 2080FD             39     JB P0.0,$                       ;Wait till Data Port LOW
003A C28E               40     CLR TR1                         ;Stop Timer 1
003C E58B               41     MOV A,TL1                       ;Move Timer 1 Low to ACC
003E 9432               42     SUBB A,#50D                     ;ACC - 50D[00110010b] If carry is set, bit is low
0040 E8                 43     MOV A,R0                        ;Move Register 0 to ACC
0041 20D704             44     JB PSW.7, LOW                   ;Jump to LOW if Carry High
0044 D2E0               45     SETB ACC.0                      ;Set ACC.0 to 1 -- High Bit Recieved
0046 8002               46     SJMP DISP                       ;Jump to DISP
                        47
                        48     LOW:
0048 C2E0               49     CLR ACC.0                       ;Clear ACC Bit 0 -- Low Bit Recieved
                        50
                        51     DISP:
004A 758B00             52     MOV TL1,#00D                    ;Set Timer 1 Low -> 0H
004D C2D7               53     CLR PSW.7                       ;Clear carry flag
004F D9DF               54     DJNZ R1,DATALOOP                ;Decrement R1, and Jump to DATALOOP if not zero
0051 20D607             55     JB PSW.6, AGAIN                 ;Jump if Decimal skip bit = 1
0054 20D50B             56     JB PSW.5, SAVETEMP              ;If F0 is 1, Save Temp
                        57     SAVEHUM:
0057 F549               58     MOV HUM, A                      ;Store Accumulator Value in HUM
0059 D2D6               59     SETB PSW.6                      ;Set Skip Decimal Data
                        60     AGAIN:
005B D2D5               61     SETB PSW.5                      ;Set Directing bit   0 = HUM   1 = TEMP
005D 7908               62     MOV R1, #08D
005F 020030             63     LJMP DATALOOP
                        64     SAVETEMP:
0062 F548               65     MOV TEMP, A                     ;Store Accumulator Value in TEMP
0064 C2D5               66     CLR PSW.5                       ;Clear Directing bit   0 = HUM   1 = TEMP
0066 C2D6               67     CLR PSW.6                       ;Clear Decimal Skip bit
                        68
                        69     DIAGNOSTIC_DISPLAY:     ;Displays Full Diagnostic screen with updating values
0068 12016D             70     LCALL DIAGTEXT                  ;Title
006B 1201F3             71     LCALL LINE2                     ;Jump to line 2
006E 1201B9             72     LCALL TEXT2                     ; RH=
0071 1200C9             73     LCALL HMDTY                     ; Hum reading
0074 1201C9             74     LCALL TEXT3                     ; T=
0077 1200F0             75     LCALL TEMPERATURE               ; Temp reading
                        76     ;LCALL DELAY2                   ; 2 sec delay
                        77     ;LJMP MAIN
007A 22                 78     RET
                        79
                        80
                        81     DELAY1:                 ;18ms Delay
007B 758CB9             82     MOV TH0,#0B9H                   ;Time high set -> 185D
007E 758AB0             83     MOV TL0,#0B0H                   ;Time low set -> 176D
0081 D28C               84     SETB TR0                        ;Timer RUN
0083 308DFD             85     JNB TF0,$                       ;Stay Here till TF0 set
0086 C28C               86     CLR TR0                         ;Clear Timer RUN
0088 C28D               87     CLR TF0                         ;Clear Timer Overflow
008A 22                 88     RET
                        89
                        90     DELAY2:                 ;******Change to timer_counter
008B 7970               91     MOV R1,#112D
                        92     BACK:
008D 12007B             93     LCALL DELAY1
0090 D9FB               94     DJNZ R1,BACK
0092 22                 95     RET
                        96
                        97     CHECK:                  ;Basic value check
0093 E8                 98     MOV A,R0
0094 75F041             99     MOV B,#65D
0097 95F0              100     SUBB A,B
0099 20D707            101     JB PSW.7,NEXT1
009C 1201C9            102     LCALL TEXT3
