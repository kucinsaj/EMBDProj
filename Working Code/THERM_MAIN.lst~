THERM_MAIN                                                                                                              PAGE 1
                         1     ;;;;;;Assign Port Names;;;;;;
  00B2                   2     RS EQU P3.2
  00B1                   3     RW EQU P3.1
  00B3                   4     E  EQU P3.3
                         5     ;;;;;;Init Variables;;;;;;
  0048                   6     TEMP EQU 48H
  0049                   7     HUM  EQU 49H
                         8     ORG 000H
                         9     ;;;;;;INIT;;;;;;
                        10     INIT:                   ;Program Initialization
0000 900209             11     MOV DPTR,#VALUES                ;Move DPTR to Values LUT
0003 D280               12     SETB P0.0                       ;Set DHT11 Data Port
0005 C2A0               13     CLR P2.0                        ;Clear Output*
0007 758921             14     MOV TMOD,#00100001B             ;Set TMOD
000A 758B00             15     MOV TL1,#00D                    ;Set Timer Low Byte
000D 1201CD             16     LCALL LCD_INIT                  ;Call LCD Init subroutine
0010 120110             17     LCALL WELCOME                   ;Call Welcome Text
                        18     ;LCALL DELAY2
0013 020207             19     LJMP    ENDP
                        20
                        21     MAIN:                   ;Main Routine
0016 7908               22     MOV R1,#8D                      ;Set R1 -> 8D ->08H
0018 D280               23     SETB P0.0                       ;Set DHT11 Data Port
001A C280               24     CLR P0.0                        ;Clear DHT11 Data Port
001C 12007A             25     LCALL DELAY1                    ;Delay
001F D280               26     SETB P0.0                       ;Set DHT11 Data Port
0021 2080FD             27     WAIT1:JB P0.0,$                 ;Wait till Data Port LOW
0024 3080FD             28     WAIT2:JNB P0.0,$                ;Wait till Data Port HIGH
0027 2080FD             29     WAIT3:JB P0.0,$                 ;Wait till Data Port LOW
                        30
                        31     DATALOOP:               ;DHT11 Data Colection LOOP
002A 3080FD             32     JNB P0.0,DATALOOP               ;Wait till Data Port HIGH
002D 23                 33     RL A                            ;Rotate ACC Left
002E F8                 34     MOV R0,A                        ;Move ACC to Register 0
002F D28E               35     SETB TR1                        ;SET Timer 1 RUN
                        36     WAIT4:
0031 2080FD             37     JB P0.0,$                       ;Wait till Data Port LOW
0034 C28E               38     CLR TR1                         ;Stop Timer 1
0036 E58B               39     MOV A,TL1                       ;Move Timer 1 Low to ACC
0038 9432               40     SUBB A,#50D                     ;ACC - 50D[00110010b] If carry is set, bit is low
003A E8                 41     MOV A,R0                        ;Move Register 0 to ACC
003B 20D704             42     JB PSW.7, LOW                   ;Jump to LOW if Carry High
003E D2E0               43     SETB ACC.0                      ;Set ACC.0 to 1 -- High Bit Recieved
0040 8002               44     SJMP DISP                       ;Jump to DISP
                        45
                        46     LOW:
0042 C2E0               47     CLR ACC.0                       ;Clear ACC Bit 0 -- Low Bit Recieved
                        48
                        49     DISP:
0044 758B00             50     MOV TL1,#00D                    ;Set Timer 1 Low -> 0H
0047 C2D7               51     CLR PSW.7                       ;Clear carry flag
0049 D9DF               52     DJNZ R1,DATALOOP                ;Decrement R1, and Jump to DATALOOP if not zero
004B 20D607             53     JB PSW.6, AGAIN                 ;Jump if Decimal skip bit = 1
004E 20D50B             54     JB PSW.5, SAVETEMP              ;If F0 is 1, Save Temp
                        55     SAVEHUM:
0051 F549               56     MOV HUM, A                      ;Store Accumulator Value in HUM
0053 D2D6               57     SETB PSW.6                      ;Set Skip Decimal Data
                        58     AGAIN:
0055 D2D5               59     SETB PSW.5                      ;Set Directing bit   0 = HUM   1 = TEMP
0057 7908               60     MOV R1, #08D
0059 02002A             61     LJMP DATALOOP
                        62     SAVETEMP:
005C F548               63     MOV TEMP, A                     ;Store Accumulator Value in TEMP
005E C2D5               64     CLR PSW.5                       ;Clear Directing bit   0 = HUM   1 = TEMP
0060 C2D6               65     CLR PSW.6                       ;Clear Decimal Skip bit
                        66
                        67     DIAGNOSTIC_DISPLAY:     ;Displays Full Diagnostic screen with updating values
0062 120161             68     LCALL DIAGTEXT                  ;Title
0065 1201EA             69     LCALL LINE2                     ;Jump to line 2
0068 1201AD             70     LCALL TEXT2                     ; RH=
006B 1200BD             71     LCALL HMDTY                     ; Hum reading
006E 1201BD             72     LCALL TEXT3                     ; T=
0071 1200E4             73     LCALL TEMPERATURE               ; Temp reading
0074 12008A             74     LCALL DELAY2                    ; 2 sec delay
0077 020016             75     LJMP MAIN
                        76
                        77
                        78     DELAY1:                 ;18ms Delay
007A 758CB9             79     MOV TH0,#0B9H                   ;Time high set -> 185D
007D 758AB0             80     MOV TL0,#0B0H                   ;Time low set -> 176D
0080 D28C               81     SETB TR0                        ;Timer RUN
0082 308DFD             82     JNB TF0,$                       ;Stay Here till TF0 set
0085 C28C               83     CLR TR0                         ;Clear Timer RUN
0087 C28D               84     CLR TF0                         ;Clear Timer Overflow
0089 22                 85     RET
                        86
                        87     DELAY2:                 ;******Change to timer_counter
008A 7970               88     MOV R1,#112D
                        89     BACK:
008C 12007A             90     LCALL DELAY1
008F D9FB               91     DJNZ R1,BACK
0091 22                 92     RET
                        93
                        94     CHECK:                  ;Basic value check
0092 E8                 95     MOV A,R0
0093 75F041             96     MOV B,#65D
0096 95F0               97     SUBB A,B
0098 20D707             98     JB PSW.7,NEXT1
009B 1201BD             99     LCALL TEXT3
